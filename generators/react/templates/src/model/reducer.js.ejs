import { combineReducers } from 'redux';
import * as ActionTypes from './action-types';
<% if(isRouter) {%>import { routerReducer } from 'react-router-redux-ie8'<%}%>
function appState(state = { data: 'Hello from redux!' }, action) {
  switch (action.type) {
    case ActionTypes.SYNC_ACTION:
      return { ...state, data: action.data };
    default:
      return state;
  }
}

export default combineReducers({ appState<% if(isRouter) {%>, routing: routerReducer<%}%> });
